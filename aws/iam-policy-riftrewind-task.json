{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ModelsAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::riftrewind-models-*",
        "arn:aws:s3:::riftrewind-models-*/*"
      ]
    },
    {
      "Sid": "SecretsManagerAccess",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetSecretValue"
      ],
      "Resource": "arn:aws:secretsmanager:*:*:secret:riftrewind/riot-api-key-*"
    }
  ]
}
